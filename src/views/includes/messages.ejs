<% if (errors.length> 0) {%>
  <div class="row">
    <div class="col">
      <div class="alert alert-danger">
        <% for (error of errors) {%>
          <%= error %><br>
            <% } %>
      </div>
    </div>
  </div>
  <% } %>

    <% if (success.length> 0) {%>
      <div class="row">
        <div class="col">
          <div class="alert alert-success">
            <%= success %><br>
          </div>
        </div>
      </div>
      <% } %>